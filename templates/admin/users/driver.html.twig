{% extends "admin/base.html.twig" %}
{% block body %}


    <div class="page-header">
        <div class="page-title">
            <h3>
                {% trans %}User{% endtrans %}
                <small>>{% trans %}Edit driver info{% endtrans %}</small>
            </h3>
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading">
            <h6 class="panel-title">
                {% trans %}Edit driver info{% endtrans %}
            </h6>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-sm-5">
                    <div class="panel panel-default">
                        <div class="panel-body">
                            {{ form_start(form) }}
                            {{ form_errors(form) }}
                            {{ form_widget(form) }}
                            <button type="submit" class="btn btn-primary">
                                {% trans %}Update{% endtrans %}</button>
                            {{ form_end(form) }}
                        </div>
                    </div>
                </div>
                <div class="col-sm-7">
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <br><br><br><br><br><br><br><br><br><br>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-body">
            <a href="{{ path('users') }}" class="btn btn-default">
                {% trans %}Back{% endtrans %}
            </a>
        </div>
    </div>

{% endblock %}
