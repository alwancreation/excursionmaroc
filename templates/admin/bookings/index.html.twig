                    <!--begin::Menu-->
                    <a href="{{ path('booking_new') }}" type="button" class="btn btn-primary btn-active-light-primary" data-kt-menu-trigger="click" data-kt-menu-placement="bottom-end">
                        <i class="fa fa-plus"></i> {% trans %}New{% endtrans %}
                    </a>
                </div>
            </div>
            <div class="card-body pt-0">
                <!--begin::Table-->
                <table class="table align-middle table-row-dashed fs-6 gy-2" id="datatable-booking">
                    <!--begin::Table head-->
                    <thead>
                    <!--begin::Table row-->
                    <tr class="text-start text-muted fw-bolder fs-7 text-uppercase gs-0">
                        <th>{% trans %}Ref{% endtrans %}</th>
                        <th>{% trans %}Client{% endtrans %}</th>
                        <th>{% trans %}Date Create{% endtrans %}</th>
                        <th>{% trans %}Date Start{% endtrans %}</th>
                        <th>{% trans %}Date End{% endtrans %}</th>
                        <th>{% trans %}Status{% endtrans %}</th>
                        <th>{% trans %}Pax{% endtrans %}</th>
                        <th class="text-end min-w-70px">{% trans %}Actions{% endtrans %}</th>
                    </tr>
                    <!--end::Table row-->
                    </thead>
                    <!--end::Table head-->
                    <!--begin::Table body-->
                    <tbody class="text-gray-600 fw-bold">
                    {% for booking in bookings %}
                        <tr>
                            <!--begin::Checkbox-->
                            <td>{{ booking.id }}</td>
                            <td>
                                <div class="d-flex flex-column">
                                    <a href="{{ path('booking_show', {'id': booking.id}) }}" class="text-gray-800 text-hover-primary mb-1">{{ booking.client }}</a>
                                    <span><a href="{{ path('booking_show', {'id': booking.id}) }}">{{ booking.clientName }}</a></span>
                                </div>
                            </td>
                            <td>{{ booking.dateCreate ? booking.dateCreate|date('d/m H:i') : '' }}</td>
                            <td>{{ booking.dateStart ? booking.dateStart|date('Y-m-d') : '' }}</td>
                            <td>{{ booking.dateEnd ? booking.dateEnd|date('Y-m-d') : '' }}</td>
                            <td>
                                <div class="badge {{ booking.statusClass }}">{{ booking.statusString|trans }}</div>
                            </td>

                            <td>
                                <div class="d-flex flex-column">
                                    <b>{% trans %}Pax{% endtrans %} : {{ booking.pax }}</b>
                                    <small>
                                        {% trans %}Adults{% endtrans %} : {{ booking.numberOfAdults }} / {% trans %}Children{% endtrans %} : {{ booking.numberOfChildren }}
                                    </small>
                                </div>



                            </td>

                            <td class="text-end">
                                <a href="#" class="btn btn-light btn-active-light-primary btn-sm" data-kt-menu-trigger="click" data-kt-menu-placement="bottom-end">Actions
                                    <!--begin::Svg Icon | path: icons/duotune/arrows/arr072.svg-->
                                    <span class="svg-icon svg-icon-5 m-0">
															<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
																<path d="M11.4343 12.7344L7.25 8.55005C6.83579 8.13583 6.16421 8.13584 5.75 8.55005C5.33579 8.96426 5.33579 9.63583 5.75 10.05L11.2929 15.5929C11.6834 15.9835 12.3166 15.9835 12.7071 15.5929L18.25 10.05C18.6642 9.63584 18.6642 8.96426 18.25 8.55005C17.8358 8.13584 17.1642 8.13584 16.75 8.55005L12.5657 12.7344C12.2533 13.0468 11.7467 13.0468 11.4343 12.7344Z" fill="black" />
															</svg>
														</span>
                                    <!--end::Svg Icon--></a>
                                <!--begin::Menu-->
                                <div class="menu menu-sub menu-sub-dropdown menu-column menu-rounded menu-gray-600 menu-state-bg-light-primary fw-bold fs-7 w-125px py-4" data-kt-menu="true">
                                    <!--begin::Menu item-->
                                    <div class="menu-item px-3">
                                        <a href="{{ path('booking_show', {'id': booking.id}) }}" class="menu-link px-3">{% trans %}Show{% endtrans %}</a>
                                    </div>
                                    <!--end::Menu item-->
                                    <!--begin::Menu item-->
                                    <div class="menu-item px-3">
                                        <a href="{{ path('booking_edit', {'id': booking.id}) }}" class="menu-link px-3">{% trans %}Edit{% endtrans %}</a>
                                    </div>
                                    <!--end::Menu item-->
                                    <!--begin::Menu item-->
                                    <div class="menu-item px-3">
                                        <a href="{{ path('booking_delete', {'id': booking.id}) }}" onclick="return confirm('Conform !')" class="menu-link px-3">{% trans %}Delete{% endtrans %}</a>
                                    </div>
                                    <!--end::Menu item-->
                                </div>
                                <!--end::Menu-->
                            </td>
                            <!--end::Action=-->
                        </tr>
                    {% endfor %}

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
