{% extends 'AdminBundle::base.html.twig' %}

{% block body %}
<!-- Page header -->
<div class="page-header">
    <div class="page-title">
        <h3>
            {% trans from 'AdminBundle' %}Vehicle edit{% endtrans %}
            <small>{% trans from 'AdminBundle' %}Vehicle edit{% endtrans %}</small>
        </h3>
    </div>
</div>
<!-- Breadcrumbs line -->
<div class="breadcrumb-line">
    <ul class="breadcrumb">
        <li><a href="{{ url('admin_default_index') }}">{% trans from 'AdminBundle' %}Home{% endtrans %}</a></li>
        <li class="active">{% trans from 'AdminBundle' %}Vehicle edit{% endtrans %}</li>
    </ul>
</div>
<div class="content">

    {{ form_start(edit_form) }}
        {{ form_widget(edit_form) }}
        <input type="submit" class="btn btn-primary" value="{% trans from 'AdminBundle' %}Edit{% endtrans %}" />
    {{ form_end(edit_form) }}
</div>
<div class="panel panel-default">
    <div class="panel-body">
        <a href="{{ path('vehicules_index') }}">{% trans from 'AdminBundle' %}Back to the list{% endtrans %}</a>
    </div>
</div>
{% endblock %}
