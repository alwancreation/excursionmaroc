{% extends '@Admin/base.html.twig' %}

{% block body %}
    <!-- Page header -->
    <div class="page-header">
        <div class="page-title">
            <h3>
                {% trans from "AdminBundle" %}New Agency{% endtrans %}
                <small>{% trans from "AdminBundle" %}New Agency{% endtrans %}</small>
            </h3>
        </div>
    </div>
    <!-- /page header -->


    <!-- Breadcrumbs line -->
    <div class="breadcrumb-line">
        <ul class="breadcrumb">
            <li><a href="{{ url('admin_default_index') }}">{% trans %}Admin Dashboard{% endtrans %}</a></li>
            <li><a href="{{ url('agency_index') }}">{% trans from "AdminBundle" %}Agencies list{% endtrans %}</a></li>
            <li class="active">{% trans from "AdminBundle" %}New Agency{% endtrans %}</li>
        </ul>
    </div>
    <div class="row">
        <div class="col-sm-6">
            <div class="panel panel-default">
            	<div class="panel-body">
                    {% form_theme form "bootstrap_3_layout.html.twig" %}
                    {{ form_start(form) }}
                    {{ form_widget(form) }}
                    <input type="submit" value="{% trans %}Create{% endtrans %}" />
                    {{ form_end(form) }}
            	</div>
            </div>


        <div class="panel panel-default">
        	<div class="panel-body">
                <a href="{{ path('agency_index') }}" class="btn btn-default">Back to the list</a>
        	</div>
        </div>

        </div>
    </div>
{% endblock %}
